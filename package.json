{
  "name": "@chezmoi-sh/argocd-extension-application-map",
  "version": "1.0.1",
  "private": false,
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@xyflow/react": "^12.6.4",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "scripts": {
    "build-storybook": "storybook build",
    "build:dev": "NODE_ENV=development pnpm webpack",
    "build:enpack": "tar -C dist -cvf dist/extension-application-map.tar resources",
    "build:prod": "NODE_ENV=production pnpm webpack",
    "build": "pnpm build:prod && pnpm build:enpack",
    "dev": "NODE_ENV=development pnpm webpack:watch",
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm dev",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test:ci": "jest --reporters=default --reporters=jest-junit --ci --coverage --testResultsProcessor=jest-junit",
    "test:watch": "jest --watch",
    "test": "jest",
    "webpack:watch": "webpack --config ./webpack.config.js --watch",
    "webpack": "webpack --config ./webpack.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^3.2.6",
    "@jest/globals": "^30.0.0",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "babel-jest": "^30.0.0",
    "babel-preset-react": "^6.24.1",
    "chromatic": "^13.0.0",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.3.0",
    "eslint-plugin-storybook": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-junit": "^16.0.0",
    "playwright": "^1.52.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.14",
    "storybook-addon-deep-controls": "^0.9.2",
    "style-loader": "^4.0.0",
    "task-master-ai": "^0.17.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  }
}
